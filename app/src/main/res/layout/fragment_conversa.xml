<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <FrameLayout android:id="@+id/frameChat"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyle">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="top|start"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                app:contentInsetStart="0dp" style="@style/groupStylebluegray_900">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_160pxh"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageArrowleft"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_marginBottom="@dimen/_18pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_arrowleft" />
                    <ImageView
                        android:id="@+id/imageSmall"
                        android:layout_width="@dimen/_50pxv"
                        android:layout_height="@dimen/_40pxv"
                        android:src="@drawable/chatuno"
                        android:layout_marginStart="@dimen/_28pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:scaleType="fitXY"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/chatuno" />
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnfrankpa"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_weight="0.48"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFrankPa"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_frankpa"
                            style="@style/txtMulishsemibold16" tools:text="@string/lbl_frankpa"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowellipseone"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <View
                                android:background="@drawable/rectangle_bg_green_500_radius_3"
                                android:id="@+id/viewEllipseOne"
                                android:layout_height="@dimen/_6pxh"
                                android:layout_marginBottom="@dimen/_3pxv"
                                android:layout_marginTop="@dimen/_4pxv" android:layout_width="@dimen/_6pxh"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtEnlinea"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_4pxh"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@string/lbl_en_linea"
                                style="@style/txtNunitomedium10" tools:text="@string/lbl_en_linea"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnholacmoHasEstadoTripleHdp"
                android:layout_gravity="end"
                android:layout_height="@dimen/_54pxv"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_28pxv"
                android:layout_width="@dimen/_252pxh"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_17pxh"
                android:paddingStart="@dimen/_17pxh"
                android:paddingTop="@dimen/_18pxv"
                android:singleLine="true"
                android:text="@string/msg_hola_c_mo_ha"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_2" tools:text="@string/msg_hola_c_mo_ha"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowbluerobotmasc"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="match_parent" android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnYepaQuePasaGenteEstoyBienMarica"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_weight="0.91"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_9pxh"
                    android:paddingStart="@dimen/_9pxh"
                    android:paddingTop="@dimen/_18pxv"
                    android:singleLine="true"
                    android:text="@string/msg_yepa_que_pasa_g"
                    android:textAllCaps="false"
                    style="@style/groupStylegray_200cornerRadius" tools:text="@string/msg_yepa_que_pasa_g"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnMeHasLlamadoMaricaTuSiQueEresMaricon"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_11pxh"
                android:paddingStart="@dimen/_10pxh"
                android:paddingTop="@dimen/_18pxv"
                android:singleLine="true"
                android:text="@string/msg_me_has_llamado"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_2" tools:text="@string/msg_me_has_llamado"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowframesixtythree"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="match_parent" android:orientation="horizontal">

                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumndescription"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_weight="0.91"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylegray_200cornerRadius">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtDescription"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginEnd="@dimen/_9pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="match_parent"
                        android:text="@string/msg_buena_idea_si"
                        style="@style/txtNunitobold13" tools:text="@string/msg_buena_idea_si"/>
                </LinearLayout>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnQueMeEstasContando"
                android:layout_gravity="end"
                android:layout_height="@dimen/_54pxv"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="@dimen/_184pxh"
                android:paddingHorizontal="@dimen/_18pxh"
                android:paddingVertical="@dimen/_18pxv"
                android:singleLine="true"
                android:text="@string/msg_que_me_estas_co"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_2" tools:text="@string/msg_que_me_estas_co"/>
            <LinearLayout android:id="@+id/linearRowescribetumens"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_58pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_1">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEscribetumens"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_weight="0.7"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/msg_escribe_tu_mens"
                    style="@style/txtNunitobold13_1" tools:text="@string/msg_escribe_tu_mens"/>
                <LinearLayout android:id="@+id/linearRowmenu"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_19pxh"
                    android:layout_marginStart="@dimen/_131pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_weight="0.3"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageMenu"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_menu"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
                    <ImageView android:id="@+id/imageSave"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_save"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_save"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
