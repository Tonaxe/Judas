<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data />

    <FrameLayout
        android:id="@+id/frameMusica"
        style="@style/groupStyle"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            style="@style/groupStylebluegray_900"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            app:contentInsetStart="0dp">

            <ImageView
                android:id="@+id/imageArrowleft"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_marginEnd="@dimen/_312pxh"
                android:layout_marginBottom="@dimen/_26pxv"
                android:src="@drawable/img_arrowleft"
                android:scaleType="fitXY"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_arrowleft" />

        </androidx.appcompat.widget.Toolbar>

        <LinearLayout
            android:id="@+id/linearColumngroupone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="top|start"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageViewAboveFerxxo"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_238pxh"
                android:layout_marginStart="@dimen/_64pxh"
                android:layout_marginTop="@dimen/_151pxv"
                android:layout_marginEnd="@dimen/_37pxh"
                android:src="@drawable/img_songcoverart"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/linearRowferxxo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_37pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_37pxh"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtFerxxo"
                    style="@style/txtMadabold20"
                    android:layout_width="0dp"
                    android:layout_marginStart="@dimen/_30pxv"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.76"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_ferxxo100"
                    tools:text="@string/lbl_ferxxo100" />

                <TextView
                    android:id="@+id/txtFeid"
                    style="@style/txtMadabold15"
                    android:layout_width="0dp"
                    android:layout_marginStart="@dimen/_70pxv"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.24"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_feid"
                    tools:text="@string/lbl_feid" />

            </LinearLayout>



            <FrameLayout
                android:id="@+id/frameStackrectanglefortythree"
                android:layout_width="@dimen/_280pxh"
                android:layout_height="@dimen/_10pxv"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_37pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_37pxh">

                <View
                    android:id="@+id/lineRectangleFortyThree"
                    android:layout_width="@dimen/_280pxh"
                    android:layout_height="@dimen/_2pxv"
                    android:layout_gravity="start"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:background="@drawable/rectangle_bg_gray_501_radius_1" />

                <View
                    android:id="@+id/viewEllipseTwenty"
                    android:layout_width="@dimen/_9pxh"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_gravity="start"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:background="@drawable/rectangle_bg_gray_200_radius_5" />
            </FrameLayout>

            <LinearLayout
                android:id="@+id/linearRowtime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_37pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_marginEnd="@dimen/_30pxh"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtTime"
                    style="@style/txtRobotoregular10"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_30pxv"
                    android:layout_weight="0.51"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_00_36"
                    tools:text="@string/lbl_00_36" />

                <TextView
                    android:id="@+id/txtTimeOne"
                    style="@style/txtRobotoregular10"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_190pxh"
                    android:layout_weight="0.49"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_05_39"
                    tools:text="@string/lbl_05_39" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRowplaybackicon"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_50pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_marginEnd="@dimen/_37pxh"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imagePlayBackIcon"
                    android:layout_width="@dimen/_24pxh"
                    android:layout_height="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:layout_marginStart="@dimen/_15pxv"
                    android:src="@drawable/img_playbackicon"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_playbackicon" />

                <ImageView
                    android:id="@+id/imageMediaskippr"
                    android:layout_width="@dimen/_12pxh"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_41pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_mediaskippr"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_mediaskippr" />

                <ImageView
                    android:id="@+id/imageThumbsup"
                    android:layout_width="@dimen/_20pxh"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_58pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_thumbsup"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_thumbsup" />

                <ImageView
                    android:id="@+id/imageMediaskipne"
                    android:layout_width="@dimen/_12pxh"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_mediaskipne"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_mediaskipne" />

                <ImageView
                    android:id="@+id/imageFavorite"
                    android:layout_width="@dimen/_15pxh"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_48pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_favorite"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_favorite" />
            </LinearLayout>

            <com.google.android.exoplayer2.ui.PlayerView
                android:id="@+id/exoPlayerView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="16dp"
                android:background="@android:color/black"
                app:use_controller="true" />
        </LinearLayout>
    </FrameLayout>
</layout>
